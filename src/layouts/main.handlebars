<html lang='en'>
	<head>
		<meta charset='UTF-8' />
		<meta name='viewport' content='width=device-width, initial-scale=1.0' />
		<title>Document</title>
		<link rel='stylesheet' href='/css/index.css' />
		<link
			href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css'
			rel='stylesheet'
			integrity='sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9'
			crossorigin='anonymous'
		/>

	</head>
	<body>
		<nav class='navbar navbar-expand-lg navbar-dark bg-dark'>
			<div class='container-fluid'>
				{{! nav tittle }}
				<a class='navbar-brand' href='/api/products/mongo'>CoderProyect</a>
				<button
					class='navbar-toggler'
					type='button'
					data-bs-toggle='collapse'
					data-bs-target='#navbarNav'
					aria-controls='navbarNav'
					aria-expanded='false'
					aria-label='Toggle navigation'
				>
					<span class='navbar-toggler-icon'></span>
				</button>
				<div class='collapse navbar-collapse' id='navbarNav'>
					<ul class='navbar-nav'>
						{{! home }}
						<li class='nav-item'>
							<a
								class='nav-link active'
								aria-current='page'
								href='/'
							>Home</a>
						</li>
						{{! productos MONGO }}
						<li class='nav-item'>
							<a
								class='nav-link'
								href='/api/products/mongo'
							>Productos</a>
						</li>
						{{! carts MONGO }}
						<li class='nav-item'>
							<a class='nav-link' href='/api/carts/mongo'>
								Carts</a>
						</li>
						{{! PRODUCTOS CON SOCKET }}
						<li class='nav-item'>
							<a class='nav-link' href='/realtimeproducts'>Products
								Socket</a>
						</li>
						{{! CHAT CON SOCKET }}
						<li class='nav-item'>
							<a class='nav-link' href='/chat'>Chat</a>
						</li>
						{{! PRODUCTOS CON FS }}
						<li class='nav-item'>
							<a class='nav-link' href='/api/products'>Productos FS</a>
						</li>
						{{! LOGIN }}
						<li class='nav-item oculto' id='nav-login'>
							<a class='nav-link' href='/api/session/login'>Login</a>
						</li>
						{{! LOGOUT }}
						<li class='nav-item oculto' id='nav-logout'>
							<a class='nav-link' href='/api/session/logout'>Logout</a>
						</li>
						{{! SIGN UP }}
						<li class='nav-item oculto' id='nav-signup'>
							<a class='nav-link' href='/api/session/signup'>Signup</a>
						</li>
						{{! PROFILE }}
						<li class='nav-item oculto' id='nav-profile'>
							<a class='nav-link' href='/api/session/current'>Profile</a>
						</li>
					</ul>
				</div>
			</div>
		</nav>
		{{! Body }}
		{{{body}}}
		<script
			src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js'
			integrity='sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm'
			crossorigin='anonymous'
		></script>

		<script src='/js/nav.js'></script>
	</body>
</html>